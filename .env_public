# --- CONFIGURATION GÉNÉRALE ---
# Choix du backend : "WEBUI" (local) ou "OPENROUTER" (cloud)
AI_PROVIDER=WEBUI

# --- CONFIGURATION WEBUI (Local) ---
# L'URL de votre serveur seer/WebUI (avec /v1)
WEBUI_BASE_URL=http://127.0.0.1:5000/v1
# Clé API (souvent "sk-dummy" en local)
WEBUI_API_KEY=sk-dummy
# Nom du modèle (laisser vide ou mettre "default")
WEBUI_MODEL_NAME=

# --- CONFIGURATION OPENROUTER (Cloud) ---
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_API_KEY=sk-or-votre-cle-ici
OPENROUTER_MODEL_NAME=google/gemini-2.0-flash-exp:free

# --- PARAMÈTRES WHISPER ---
# Tailles dispos : tiny, base, small, medium, large-v2
WHISPER_MODEL_SIZE=small
COMPUTE_TYPE=float16

# --- DONNEES ---
VOCAB_FILE=